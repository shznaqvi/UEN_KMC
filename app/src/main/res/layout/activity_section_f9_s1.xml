                    </RadioGroup>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!-- f9203  CheckBox   -->
            <androidx.cardview.widget.CardView
                android:id="@+id/fldGrpCVf9203"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="@string/Q_f9203" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="@string/f9203" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/f9203check"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:tag="0">

                        <CheckBox
                            android:id="@+id/f9203a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203a" />

                        <CheckBox
                            android:id="@+id/f9203b"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203b" />

                        <CheckBox
                            android:id="@+id/f9203c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203c" />

                        <CheckBox
                            android:id="@+id/f9203d"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203d" />

                        <CheckBox
                            android:id="@+id/f9203e"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203e" />

                        <CheckBox
                            android:id="@+id/f9203f"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203f" />

                        <CheckBox
                            android:id="@+id/f9203g"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203g" />

                        <CheckBox
                            android:id="@+id/f9203h"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203h" />

                        <CheckBox
                            android:id="@+id/f9203i"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9203i" />

                        <CheckBox
                            android:id="@+id/f920396"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f920396" />

                        <EditText
                            android:id="@+id/f920396x"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:digits="AaBbCcDdEeFfGgHhIiJjKkLlMmNnOoPpQqRrSsTtUuVvWwXxYyZz0123456789,. "
                            android:hint="@string/f920396"
                            android:tag="f920396"
                            android:text='@{f920396.checked ? f920396x.getText().toString() : ""}'
                            android:visibility='@{f920396.checked? View.VISIBLE : View.GONE}' />
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!-- f9204  Radio   -->
            <androidx.cardview.widget.CardView
                android:id="@+id/fldGrpCVf9204"
                style="@style/cardView">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bottom"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/quesNum"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="@string/Q_f9204" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="@string/f9204" />
                    </LinearLayout>

                    <RadioGroup
                        android:id="@+id/f9204"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RadioButton
                            android:id="@+id/f9204h"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9204h" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/f9204hx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/f9204h"
                            android:inputType="number"
                            android:maxLength="3"
                            android:tag="f9204h"
                            android:text='@{f9204h.checked ? f9204hx.getText().toString() : ""}'

                            android:visibility='@{f9204h.checked? View.VISIBLE : View.GONE}'
                            app:maxValue="100"
                            app:type="range" />

                        <RadioButton
                            android:id="@+id/f9204m"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9204m" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/f9204mx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/f9204m"
                            android:inputType="number"
                            android:maxLength="3"
                            android:tag="f9204m"
                            android:text='@{f9204m.checked ? f9204mx.getText().toString() : ""}'

                            android:visibility='@{f9204m.checked? View.VISIBLE : View.GONE}'

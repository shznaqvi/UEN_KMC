                            android:text="@string/f9201" />
                    </LinearLayout>

                    <RadioGroup
                        android:id="@+id/f9201"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RadioButton
                            android:id="@+id/f9201a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/f9201a" />

                        <RadioButton
                            android:id="@+id/f9201b"
